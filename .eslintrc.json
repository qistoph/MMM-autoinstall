{
	"extends": "eslint:recommended",
	"rules": {
		"indent": ["error", "tab"],
		"quotes": ["error", "double"],
		"max-len": ["error", 250],
		"curly": "error",
		"camelcase": ["error", {"properties": "never"}],
		"no-console": ["off"],
		"no-unused-vars": ["error", {"args": "none"}]
	},
	"env": {
		"browser": true,
		"node": true,
		"es6": true
	}
}
